{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "rules": {
      "require-jsdoc": "off",
      "no-console": "warn",
      "indent": ["error", 2],
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
    },
    "settings": {
      "react": {
      "version": "detect"
      }
    }
  }
  